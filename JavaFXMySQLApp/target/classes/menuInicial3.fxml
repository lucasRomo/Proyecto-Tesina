<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml" fx:controller="app.controller.RegistrarNuevoEmpleadoController">
    <children>
        <Label text="Usuario:"/>
        <TextField fx:id="UsuarioField" promptText="Ingrese su Usuario"/>
        <Label text="Contraseña:"/>
        <TextField fx:id="ContraseniaField" promptText="Ingrese su Contraseña"/>
        <Button text="Guardar" onAction="#guardarUsuario"/>
    </children>
</VBox>
