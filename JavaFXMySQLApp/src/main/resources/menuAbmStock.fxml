<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import java.net.URL?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.MenuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <Label text="Menu stock" styleClass="titulo-principal">
        <font>
            <Font name="System Bold" size="24.0" />
        </font>
    </Label>

    <Button styleClass="button" onAction="#handleProveedor" prefWidth="200.0" text="Proveedores" />
    <Button styleClass="button" onAction="#handleStock" prefWidth="200.0" text="Stock" />
    <Button fx:id="volverButton" layoutX="1175.0" layoutY="387.0" mnemonicParsing="false" onAction="#handleVolverButtonStock" text="Volver" />

</VBox>
