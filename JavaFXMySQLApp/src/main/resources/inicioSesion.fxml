<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.IniciodeSesionController">
    <children>
        <Label text="Inicio de Sesión">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Label text="Usuario:" />
        <TextField fx:id="UsuarioField" promptText="Ingrese su Usuario" />
        <Label text="Contraseña:" />
        <TextField fx:id="ContraseniaField" promptText="Ingrese su Contraseña" />
        <Button onAction="#IniciarSesion" text="Iniciar Sesion" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>